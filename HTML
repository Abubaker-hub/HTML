        <h1>Creating tables</h1>
        <!-- How to add tables in html -->
        <table>
            <caption>Student Details</caption>
            <tr>
                <th colspan="2">Name</th>
                <th colspan="2">Age</th>
                <th colspan="2">Designation</th>
                <th>Fav Language</th>
            </tr>
            <tr>
                <td colspan="2">Abubaker</td>
                <td rowspan="3">19</td>
                <td colspan="2">Programmer</td>
                <td>Python</td>
            </tr>
        </table><br><br>
        <table border="1">
            <thead><tr><th>Name</th><th>Age</th></tr></thead>
            <tbody><tr><td>Alice</td><td>25</td></tr></tbody>
            <caption>My Table</caption>
            <caption><details>My Table</details></caption>
        </table>




        <h1>Creating lists</h1>
        <!-- How to add lists -->
        <ul type="square">
            <li>Abubaker</li>
            <li>Yasa</li>
            <li>Hane</li>
        </ul>
        <ol type="a">
            <li>Abubaker</li>
            <li>Yasa</li>
            <li>Hane</li>
        </ol>
        <dl>
            <dt>HTML</dt>
            <dd>Lorem ipsum dolor sit amet, consectetur adipisicing elit. Nobis numquam deserunt ducimus perspiciatis. Veniam sed repellendus eos ipsam, sunt ut?</dd>
        </dl>





        <h1>Creating Forms</h1>
        <!-- Forms and input tags in HTML  -->
        <form action="post">
            <div class="name"></div>
            <label for="name">Enter your name:</label>
            <input type="text" id="name" placeholder="Enter your name" autofocus><br><br>
            
            </div>
            <div class="password">
            <label for="password">Enter your password:</label>
            <input type="password" id="password" placeholder="Enter your password"><br><br>
            </div>
            <div class="gmail">
                <label for="gmail">Enter your gmail:</label>
                <input type="email" id="gmail" placeholder="Enter your gmali"><br><br>
            </div>
            <div class="number">
                <label for="number">Enter your number:</label>
                <input type="number" id="number" placeholder="Enter your number"><br><br>
            </div>
            <div class="checkbox">
                <input type="checkbox" name="Hobby"><label>Pakistan</label>
                <input type="checkbox" name="Hobby"><label>India</label>
                <input type="checkbox" name="Hobby"><label>Dubai</label>
                <input type="checkbox" name="Hobby"><label>USA</label>
            </div>
            <div class="radio">
                <input type="radio" name="Gender"><label>Male</label>
                <input type="radio" name="Gender"><label>Female</label>
                <input type="radio" name="Gender"><label>Other</label>
            </div>
            <div class="textarea">
                <label for="comment">Enter your comments:</label><br>
                <textarea id="comment" name="comment" rows="4" cols="50">Enter your comments here...</textarea><br><br>
            </div>
            <div class="select">
                <select name="fruits">
                    <option value="apple">Apple</option>
                    <option value="banana">Banana</option>
                    <optiotn value="Orange">Orange</optiotn>
                    <option value="cherry">Cherry</option>
                    <option value="orange">Orange</option>
                    <option value="aaa">berry</option>
                </select>
            </div>
        </form>]





        <!-- Inline & Block Elements in HTML -->
        <h1>Inline & Block Elements</h1>
        <h2>Inline Elements</h2>
        <a href="https://google.com">Google</a>
        <span>Hey I am span and I am a inline Element</span><a href="">yes he is inline</a>
        
        <h2>Block Elements</h2>
        <p>Hey I am an block Element</p>
        <div>Hey I am div and I am a block Element</div>




        <!-- Id & Classes in HTML -->
        <h1>Id & Classes in HTML</h1>
        <div id="firstdiv" class="red"></div>
        <div id="seconddiv" class="red"></div>
        <span id="firstspan" class="red"></span>
        <span id="secondspan" class="red"></span>





        <!-- Video, Audio & Media in HTML -->
